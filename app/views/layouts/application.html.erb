	<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">

    <%= csrf_meta_tags %>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
</head>
<body>
	<!-- nav bar -->
	<nav class="navbar navbar-inverse navbar-fixed-top" id="my-navbar">
		<div class="container">
			<!-- <p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p> -->
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a href="" class="navbar-brand">HealthFinder</a>
			</div>
			<div class="collapse navbar-collapse" id="navbar-collapse">
				<a href="" class="btn btn-info navbar-btn navbar-right">Sign in</a>
				<ul class="nav navbar-nav">
					<li><a href="#stats">Stats</a>
					<li><a href="">Map</a>
					<li><a href="">Stories</a>

				</ul>
			</div>
		</div>
	</nav>
	<!-- Jumbotron -->
	<div class="jumbotron">
		<div class="container text-center">
			<h1>HealthfinderNYC</h1>
		</div>
	</div>
<div class="container">
	<section>
		<div class="page-header" id="stats">
			<h2>Stats</h2>
		</div>
			
	<div class="row">
			<div class="col-md-4">
					<p><ul>
						<li>Approximately 129,000 people are diagnosed with HIV/AIDS in New York State.</li>
						<li> Almost 80% live in New York City.</li>
						<li>Almost 4,000 New Yorkers will be infected with HIV this year.</li>
						</ul>
					</p>
 			</div>
 					
 			<div class="col-md-4">
				<p><ul>
					<li>New York leads the nation in the number of new HIV cases.</li>
					<li>In the first half of 2011, one-third of new HIV/AIDS diagnoses among males in New York City were in young men aged 20-29.</li>
					<li>93% of new HIV/AIDS diagnoses among females in New York City were in black or Hispanic women in the first half of 2011.</li>
					</ul>
				</p>
 			</div>

 			<div class="col-md-4">
				<p><ul>
					<li>77% of people living with HIV in New York are over forty. 42% of positive New Yorkers are over fifty.</li>
					<li>In 2010 nearly 1700 people in New York City died from AIDS-related causes.</li>
					<li>To date, over 100,000 New Yorkers have died from AIDS-related causes.</li>
				</p>
 			</div>
 	</div> <!-- End Row -->
</section>
<!-- </div> -->
<div class="well"> <!-- Twitter feed -->
	<section>
		<a class="twitter-timeline" href="https://twitter.com/hiv_nyc" data-widget-id="572885790218452993">Tweets by @hiv_nyc</a> <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
		
	</section>
</div> <!--  End Twitter feed -->

<%= yield %>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
